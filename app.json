{
  "expo": {
    "name": "$EXPO_PUBLIC_APP_NAME",
    "slug": "bytebank-app",
    "scheme": "bytebank",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./contents/figma/icons/Logo.png",
    "splash": {
      "image": "./contents/figma/icons/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bytebank.ios",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.bytebankapp.android"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./contents/figma/icons/Favicon 64px.png"
    },
    "plugins": [
      "expo-apple-authentication",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.102802199932-3c8av88ho09numo7u87evflujm83v3sn",
          "androidUrlScheme": "com.googleusercontent.apps.102802199932-lnv0non6dphbc4i6r8rrd4motkct34gq.apps.googleusercontent.com"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "$EXPO_PUBLIC_PROJECT_ID"
      },
      "useMock": "$EXPO_PUBLIC_USE_MOCK",
      "version": "$EXPO_PUBLIC_APP_VERSION",
      "firebase": {
        "apiKey": "$EXPO_PUBLIC_FIREBASE_API_KEY",
        "authDomain": "$EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN",
        "projectId": "$EXPO_PUBLIC_FIREBASE_PROJECT_ID",
        "appId": "$EXPO_PUBLIC_FIREBASE_APP_ID",
        "storageBucket": "$EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET",
        "messagingSenderId": "$EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID",
        "databaseURL": "$EXPO_PUBLIC_FIREBASE_DATABASE_URL"
      }
    }
  }
}
