{
  "name": "bytebank-app",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "EXPO_PUBLIC_USE_MOCK=true npx expo start --clear",
    "start:firebase": "EXPO_PUBLIC_USE_MOCK=false npx expo start",
    "start:mocks": "EXPO_PUBLIC_USE_MOCK=true npx expo start",
    "prebuild": "npx expo prebuild",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "typecheck": "npx tsc --noEmit",
    "build": "npx tsc -p .",
    "migrate:icons": "node scripts/migrate-icons.js",
    "clear-cache": "expo start --clear",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:release": "eas build --platform android --profile production",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:release": "eas build --platform ios --profile production",
    "config:get": "npx expo config --json",
    "config:set": "dotenv -e .env -- npx expo config --type public"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.27",
    "expo": "^54.0.0",
    "expo-apple-authentication": "~8.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-web-browser": "~15.0.7",
    "firebase": "^10.12.0",
    "react": "^18.2.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "zustand": "^4.5.2",
    "react-native": "0.81.4"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "babel-preset-expo": "~54.0.0",
    "typescript": "^5.4.0"
  }
}
