{
  "name": "bytebank-app",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "EXPO_PUBLIC_USE_MOCK=false npx expo start",
    "start:mocks": "EXPO_PUBLIC_USE_MOCK=true npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "typecheck": "npx tsc --noEmit",
    "build": "npx tsc -p .",
    "migrate:icons": "node scripts/migrate-icons.js",
    "clear-cache": "expo start --clear",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:release": "eas build --platform android --profile production",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:release": "eas build --platform ios --profile production"
  },
  "dependencies": {
    "expo": "^54.0.6",
    "expo-apple-authentication": "^7.2.4",
    "expo-auth-session": "^5.5.1",
    "expo-constants": "^16.0.2",
    "expo-web-browser": "^13.0.3",
    "firebase": "^10.12.0",
    "react": "18.2.0",
    "react-native": "^0.76.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native-stack": "^6.9.27",
    "react-native-screens": "^3.31.1",
    "react-native-safe-area-context": "4.10.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "18.2.66",
    "typescript": "^5.4.0",
    "babel-preset-expo": "^14.0.0"
  }
}
